<h1>Skill Admin Page</h1>
<%= link_to "Create", new_skill_path, class: 'btn btn-success' %>
<% @skill.each do |s| %>
  <li>
    <%= s.title %>
    <%= link_to "View", s, class: 'btn btn-primary' %>
    <%= link_to "Edit", edit_skill_path(s), class: 'btn btn-success' %>
    <%= link_to "Delete", s, method: :delete, class: 'btn btn-danger' %>
  </li>
<% end %>