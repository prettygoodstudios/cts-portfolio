<h1><%= @skill.title %></h1>
<div class="progress-bar progress-bar-striped bg-warning" role="progressbar" aria-valuenow="<%= @skill.percent_utilized %>" aria-valuemax="100" style="width: <%= @skill.percent_utilized %>%"0></div>
<%= link_to "Edit", edit_skill_path(@skill), class: 'btn btn-success' %>
<%= link_to "Delete", @skill, class: 'btn btn-danger', method: :delete %>
<%= link_to "Back", skills_path, class: 'btn btn-danger' %>