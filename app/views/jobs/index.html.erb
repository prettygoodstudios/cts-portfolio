<h1>Jobs Admin Page</h1>
<%= link_to "Create", new_job_path, class: 'btn btn-success' %>
<ul>
  <% @jobs.each do |j| %>
    <li>
      <%= j.title %>
      <%= link_to "View", j, class: 'btn btn-primary' %>
      <%= link_to "Edit", edit_job_path(j), class: 'btn btn-success' %>
      <%= link_to "Delete", j, method: :delete, class: 'btn btn-danger' %>
    </li>
  <% end %>
</ul>