<%= form_with(model: project, local: true) do |form| %>
  <% if project.errors.any? %>
      <% project.errors.full_messages.each do |error| %>
        <%= alert_generator error %>
      <% end %>
  <% end %>
  <div class="form-group">
    <%= form.label :title %>
    <%= form.text_field :title, id: :project_title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :description %>
    <%= form.text_area :description, id: :project_description, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :url %>
    <%= form.text_field :url, id: :project_url, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= form.submit "Submit", class: "btn btn-primary"%>
  </div>
<% end %>
<br>
<%= link_to "Back", projects_path, class: "btn btn-danger" %>