<div class="inner cover" style="margin-bottom: 100px;">
  <h1 class="cover-heading">Rails News</h1>
  <div class="row">
    <% @tweets.each do |tweet|%>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>